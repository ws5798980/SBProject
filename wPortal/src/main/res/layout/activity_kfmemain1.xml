<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#353535">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:elevation="1dp">
            <LinearLayout
                android:id="@+id/btn_serch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp"
                android:paddingBottom="@dimen/margin"
                android:paddingLeft="@dimen/margin"
                android:paddingRight="@dimen/marginx2"
                android:paddingTop="@dimen/margin" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/icon_search_white" />
            </LinearLayout>
            <!--<ImageView-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_centerVertical="true"-->
                <!--android:layout_marginLeft="20dp"-->
                <!--android:background="@drawable/icon_search_white"/>-->
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_centerInParent="true">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/top_location"/>
                <TextView
                    android:id="@+id/_addresstitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="서울특별시 영등포구영등포동"
                    android:textSize="12sp"
                    android:textColor="#ffffff"
                    android:textStyle="bold"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/icon_down"
                    android:layout_gravity="center_vertical"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/icon_live_bottom"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="-3dp"/>
                <ImageView
                    android:id="@+id/btn_scan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/icon_scan"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#000000"/>
        </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="top">
        <LinearLayout
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/dial_num"
            android:gravity="left"
            android:orientation="horizontal"
            android:focusable="true"
            android:focusableInTouchMode="true">
            <EditText
                android:id="@+id/topNumber1txt"
                android:layout_width="52dp"
                android:layout_height="49dp"
                android:gravity="center"
                android:textSize="27dp"
                android:inputType="number"
                android:maxLength="2"
                android:text="1"
                android:background="@null"
                android:imeOptions="actionNext"
                android:singleLine="true"
                android:nextFocusDown="@id/topNumber2txt"/>
            <EditText
                android:layout_marginLeft="7dp"
                android:id="@+id/topNumber2txt"
                android:layout_width="49dp"
                android:layout_height="49dp"
                android:gravity="center"
                android:textSize="27dp"
                android:inputType="number"
                android:maxLength="2"
                android:text="1"
                android:background="@null"/>
            <EditText
                android:layout_marginLeft="7dp"
                android:id="@+id/topNumber3txt"
                android:layout_width="49dp"
                android:layout_height="49dp"
                android:gravity="center"
                android:textSize="27dp"
                android:inputType="number"
                android:maxLength="2"
                android:text="1"
                android:background="@null"/>
            <EditText
                android:layout_marginLeft="7dp"
                android:id="@+id/topNumber4txt"
                android:layout_width="49dp"
                android:layout_height="49dp"
                android:gravity="center"
                android:textSize="27dp"
                android:inputType="number"
                android:maxLength="2"
                android:text="1"
                android:background="@null"/>
            <EditText
                android:layout_marginLeft="7dp"
                android:id="@+id/topNumber5txt"
                android:layout_width="70dp"
                android:layout_height="49dp"
                android:gravity="center"
                android:textSize="27dp"
                android:inputType="number"
                android:maxLength="3"
                android:text="100"
                android:background="@null"/>
        </LinearLayout>
        <TextView
            android:id="@+id/dial_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/dial_btn"
            android:textColor="#ffffff"
            android:gravity="center"
            android:text="선택" />
        </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="230dp"
        android:orientation="vertical"
        android:background="#353535"
        android:gravity="center">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="300dp"
                android:gravity="center_horizontal">
                <ImageView
                    android:id="@+id/btn1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon01"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon02"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon03"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon04"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon05"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="여행"
                    android:textSize="12dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="       가정"
                    android:textSize="12dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="        건축"
                    android:textSize="12dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="       교육"
                    android:textSize="12dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="        금융"
                    android:textSize="12dp"/>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal">
                <ImageView
                    android:id="@+id/btn6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon06"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon07"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon08"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon09"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon10"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="종교"
                    android:textSize="12dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="       미용"
                    android:textSize="12dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="        법률"
                    android:textSize="12dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="       쇼핑"
                    android:textSize="12dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="        언론"
                    android:textSize="12dp"/>
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal">
                <ImageView
                    android:id="@+id/btn11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon11"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon12"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon13"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon14"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
                <ImageView
                    android:id="@+id/btn15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/m_icon15"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp"/>
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="건강"
                    android:textSize="12dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="       음식"
                    android:textSize="12dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="        취미"
                    android:textSize="12dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="       컴퓨터"
                    android:textSize="12dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="        교통"
                    android:textSize="12dp"/>
            </TableRow>
        </TableLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#ffffff">

        <ListView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/event_store_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>

</LinearLayout>

