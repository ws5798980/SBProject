<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <include layout="@layout/toolbar_rt_base1"
        />
<LinearLayout android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="1dp"
    android:orientation="horizontal">
    <TextView android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:padding="@dimen/marginx2"
        android:background="@color/white"
        android:gravity="center"
        android:textColor="@color/textcolor_dp_333"
        android:textSize="@dimen/text_size_m"
        android:text="@string/dp_text_001"
        android:id="@+id/textView1"/>
    <View android:layout_width="1dp"
        android:layout_height="match_parent"/>
    <TextView android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:padding="@dimen/marginx2"
        android:background="@color/white"
         android:textSize="@dimen/text_size_m"
        android:gravity="center"
        android:textColor="@color/textcolor_dp_333"
        android:text="@string/dp_text_002"
        android:id="@+id/textView2"/>
</LinearLayout>
        <ListView android:id="@+id/lv"
            android:layout_marginTop="@dimen/marginx2"
            android:layout_width="match_parent"
            android:dividerHeight="10dp"
            android:layout_height="match_parent"
            
            ></ListView>
</LinearLayout>
